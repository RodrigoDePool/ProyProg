all: snaketest

snaketest: snaketest.o interface.o snake.o rectangles.o
	gcc -o -lpthread snaketest snaketest.o interface.o snake.o rectangles.o

snaketest.o: snaketest.c
	gcc -c -g snaketest.c

snake.o: snake.c snake.h
	gcc -c -g -pthread snake.c

interface.o: ../../interface.c ../../interface.h
	gcc -c -g ../../interface.c

rectangles.o: ../../rectangles.c ../../rectangles.h
	gcc -c -g ../../rectangles.c

clean:
	rm *.o
